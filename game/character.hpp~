




#ifndef __MUD_CHARACTER_HEAD__
#define __MUD_CHARACTER_HEAD__


#include <stdint.h>
#include <string>




namespace Fly
{

    typedef enum __character_quality__
    {
     green,		//绿色
     blue,		//蓝色
     purple,		//紫色
     orange,		//橙色
     red,		//红色
     gold		//金色
     
    } CharacterQuality;
  

    class Character
    {
    private:
      ::std::string name;//名字

    public:
      uint32_t level;//等级
      CharacterQuality quality;

      Character();
      ~Character();
    }

      
}




#endif



